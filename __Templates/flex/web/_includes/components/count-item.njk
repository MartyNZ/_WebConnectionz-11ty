{% for item in counts %}
<div class="col-lg-3 col-md-6">
  <div class="count-box">
    <i class="bi bi-{{item.icon}}"></i>
    <div>
      <span data-purecounter-start="0" data-purecounter-end="{{item.count}}" data-purecounter-duration="1" class="purecounter"></span>
      <p>{{item.title}}</p>
    </div>
  </div>
</div>
{% endfor %}